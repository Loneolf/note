  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "serve": "npm run dev",
    "dev": "cross-env NODE_ENV=development buildType=Browser rspack serve --config ./config/rspack.config.js",
    "testBuild": "cross-env NODE_ENV=production buildType=file rspack --config ./config/rspack.config.js && npm run testUpload",
    "build": "cross-env NODE_ENV=production buildType=file rspack --config ./config/rspack.config.js && npm run upLoad",
    "pBuild": "cross-env NODE_ENV=production buildType=file rspack --config ./config/rspack.config.js",
    "gitBuild": "cross-env NODE_ENV=production buildType=Browser rspack --config ./config/rspack.config.js",
    "testUpload": "cross-env upType=docTest up=full node ./src/util/upCDN.js",
    "upLoad": "cross-env upType=doc up=full node ./src/util/upCDN.js",
    "uploadCase": "cross-env upType=case up=increment node ./src/util/upCDN.js",
    "fullUploadCase": "cross-env upType=case up=full node ./src/util/upCDN.js",
    "uploadBgImg": "cross-env upType=bgimg up=increment node ./src/util/upCDN.js",
    "fullUploadBgImg": "cross-env upType=bgimg up=full node ./src/util/upCDN.js",
    "readMdFile": "cross-env readFile=md node ./src/util/dealCatalog.js",
    "readCaseFile": "cross-env readFile=case node ./src/util/dealCatalog.js",
    "readImgBg": "cross-env readFile=bgimg node ./src/util/dealCatalog.js",
    "dll": "rspack --config ./config/rspack.dll.config.js",
    "tsi": "tsc --init"
  },
